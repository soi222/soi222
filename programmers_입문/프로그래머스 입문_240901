#코딩테스트 입문
#배열 두 배 만들기
def solution(numbers):
    res = []
    for i in numbers:
        res.append(i*2)
    return res

#다른 방법
def solution(numbers):
  return [num*2 for num in numbers]

#중앙값 구하기
def solution(array):
    array = sorted(array)
    half = int(len(array)/2)
    answer = array[half]
    return answer

#최빈값 구하기
dic = {}
def solution(array):
    for i in array:
        value = array.count(i)
    dic[i] = value
    
    if max(dic.items()):
        return max(dic.items())
#틀린 거 알지마는 진득하게 고민해보기
